{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AiProviders": {
    "Default": "AzureOpenAI",
    "AzureOpenAI": {
      "Endpoint": "https://your-resource.openai.azure.com/",
      "Deployment": "gpt-4o-mini"
    },
    "OpenAI": {
        "ApiKey": "your-api-key",
        "Model": "gpt-4.1"
    },
    "Ollama": {
      "Endpoint": "http://localhost:11434",
      "Model": "llama3"
    }
  },
  "Processing": {
    "BatchSize": 100,
    "MaxParallelism": 4,
    "TimeoutSeconds": 30
  }
}
